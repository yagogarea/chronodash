          ]
        }
      }
    },
    "WW3": {
      "name": "Wave Watch III",
      "type": "wave",
      "grids": {
        "Galicia": {
          "resolution": "0.05°",
          "execution_start": [
            "00:00 UTC",
            "12:00 UTC"
          ],
          "execution_end_approx": [
            "05:00 UTC",
